pipeline {
  agent {
    label 'agent1'
  }
  stages {
    stage('build') {
      steps {        
        sh 'cd Task06/docker-encrypt'
        sh 'pwd'
        sh 'ls'        
        sh 'docker build --tag="encrypt-test" .'
        sh 'docker run 8081:80 encrypt-test:latest'
      }
    }          
  }
}

